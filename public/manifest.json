{
    "name": "柠檬新标签页",
    "version": "1.0",
    "manifest_version": 3,
    "description": "一个极简标签页.",
    "chrome_url_overrides": {
        "newtab": "index.html"
    },
    "icons": {
        "16": "icons/16.png",
        "32": "icons/32.png",
        "96": "icons/96.png",
        "128": "icons/128.png"
    },
    "content_security_policy": {
        "extension_pages": "script-src 'self'; object-src 'self';"
    }
}